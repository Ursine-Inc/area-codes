{
  "name": "mobile",
  "version": "0.0.1",
  "private": true,
  "main": "expo-router/entry",
  "dependencies": {
    "expo": "~52.0.16",
    "expo-constants": "^17.1.7",
    "expo-linking": "^7.1.7",
    "expo-router": "^5.1.3",
    "expo-status-bar": "~2.2.3",
    "react": "18.3.1",
    "react-native": "0.73.11",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "4.18.0",
    "react-native-svg": "~15.11.2",
    "react-native-svg-transformer": "~1.5.1"
  },
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start:devclient": "expo start --dev-client",
    "dev-build": "eas build --platform android --profile development --local",
    "install-dev-build": "OUTPUT_FILE=$(find . -type f -name \"*.apk\" -print0 | xargs -0 ls -t | head -n1) && adb install -r \"$OUTPUT_FILE\"",
    "rebuild": "pnpm dev-build && pnpm install-dev-build && pnpm start:devclient",
    "rebuild:preview": "eas build --platform android --profile preview --local"
  }
}
