{
  "name": "mobile",
  "root": "apps/mobile",
  "sourceRoot": "apps/mobile/src",
  "projectType": "application",
  "tags": ["scope:mobile"],
  "namedInputs": {
    "default": ["{projectRoot}/**/*"],
    "production": ["!{projectRoot}/**/*.spec.tsx"]
  },
  "targets": {
    "start": {
      "executor": "@nx/expo:start",
      "options": {
        "port": 25000,
        "devClient": false,
        "clear": true
      }
    },
    "build-preview": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "all",
        "profile": "preview"
      }
    },
    "build-ios-preview": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "ios",
        "profile": "preview"
      }
    },
    "build-android-preview": {
      "executor": "@nx/expo:build",
      "options": {
        "platform": "android",
        "profile": "preview"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/mobile",
        "main": "apps/mobile/src/index.ts",
        "tsConfig": "apps/mobile/tsconfig.app.json"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "options": {
        "jestConfig": "apps/mobile/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "lintFilePatterns": ["apps/mobile/**/*.ts", "apps/mobile/**/*.tsx"]
      }
    }
  }
}
